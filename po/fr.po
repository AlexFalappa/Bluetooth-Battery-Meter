#: ui/general.ui:9
msgid "Indicator Settings"
msgstr "Paramètres de l'indicateur"

#: ui/general.ui:12
msgid "Enable Bluetooth device battery indicators"
msgstr "Activer les indicateurs de batterie des périphériques Bluetooth"

#: ui/general.ui:13
msgid "Show icon in system tray with device battery level"
msgstr "Affiche une icône dans la barre d'état système avec le niveau de batterie du périphérique"

#: ui/general.ui:24
msgid "Show percentage in text"
msgstr "Afficher le niveau en pourcentage"

#: ui/general.ui:25
msgid "Add text in percentage next to battery indicators icon"
msgstr "Ajoute le niveau de charge en pourcentage à côté des icônes des indicateurs de batterie"

#: ui/general.ui:39
msgid "Quick Menu Settings"
msgstr "Paramètres du menu rapide"

#: ui/general.ui:42
msgid "Show Bluetooth device battery level icon"
msgstr "Afficher l'icône du niveau de batterie du périphérique Bluetooth"

#: ui/general.ui:43
msgid "Displays icon in popup menu showing Bluetooth device battery level"
msgstr "Affiche une icône dans le menu indiquant le niveau de batterie du périphérique Bluetooth"

#: ui/general.ui:54
msgid "Show Bluetooth device battery percentage"
msgstr "Afficher le pourcentage de batterie du périphérique Bluetooth"

#: ui/general.ui:55
msgid "Displays Bluetooth device battery level percentage in text"
msgstr "Affiche le niveau de charge du périphérique Bluetooth en pourcentage"

#: ui/general.ui:66
msgid "Display battery level icon before text"
msgstr "Afficher l'icône du niveau de batterie avant le texte"

#: ui/general.ui:67
msgid "Swaps position of battery level icon and percentage text"
msgstr "Interverti la position de l'icône du niveau de batterie et du texte en pourcentage"

